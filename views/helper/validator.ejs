<script>

	function limitPhone() {
		if(!((event.keyCode>=48&&event.keyCode<=57)||(event.keyCode>=96&&event.keyCode<=105) || event.keyCode==13 || event.keyCode==8))
		event.preventDefault();
	}

	function limitIP() {
		if(!((event.keyCode>=48&&event.keyCode<=57)||(event.keyCode>=96&&event.keyCode<=105) || event.keyCode==110 || event.keyCode==13 || event.keyCode==8))
		event.preventDefault();
	}
	
</script>